{
	"name": "nox-php/ruby",
	"description": "A red LastChaos game server theme.",
	"keywords": [
		"nox",
		"last-chaos",
		"nox-theme"
	],
	"require": {
		"php": "^8.1",
		"laravel/framework": "^9.46"
	},
	"autoload": {
		"psr-4": {
			"Nox\\Ruby\\": "src"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Nox\\Ruby\\RubyServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
